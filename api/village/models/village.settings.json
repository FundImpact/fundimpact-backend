{
  "kind": "collectionType",
  "collectionName": "villages",
  "info": {
    "name": "Village"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "block": {
      "model": "block",
      "via": "villages"
    },
    "geo_regions": {
      "via": "village_id",
      "collection": "geo-regions"
    }
  }
}
