{
  "kind": "collectionType",
  "collectionName": "budget_category_organizations",
  "info": {
    "name": "Budget Category Organization"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "organization": {
      "via": "budget_category_organizations",
      "model": "organization"
    },
    "deleted": {
      "type": "boolean",
      "default": false
    },
    "project_id": {
      "via": "budget_category_organizations",
      "model": "project"
    },
    "project_with_deliverable_targets": {
      "via": "category",
      "collection": "project-with-deliverable-target"
    }
  }
}
